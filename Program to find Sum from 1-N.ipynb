   "outputs": [],
   "source": [
    "n=int(input(\"Enter a no. upto which you want sum\"))\n",
    "i=1\n",
    "sum=0\n",
    "while(i<=n):\n",
    "    sum=sum+i\n",
    "    i=i+1\n",
    "print(\"Sum=\",sum)"
